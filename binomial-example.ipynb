   "source": [
    "from scipy.stats import binom, nbinom\n",
    "from dsbay.distribution import DistributionMass\n",
    "\n",
    "N = 34\n",
    "z = 11\n",
    "theta = 0.5\n",
    "x = [_z for _z in range(1,N+1)]\n",
    "y = [binom.pmf(_z, N, theta) for _z in range(1,N+1)]\n",
    "\n",
    "d = DistributionMass(x,y)\n",
    "ax = d.plot()\n",
    "_ = ax.set_xlabel(\"Number of heads (z)\")"
   ]
  }
