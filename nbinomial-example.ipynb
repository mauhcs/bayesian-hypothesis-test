   "source": [
    "from scipy.stats import nbinom\n",
    "from dsbay.distribution import DistributionMass\n",
    "\n",
    "N = 34\n",
    "z = 11\n",
    "theta = 0.5\n",
    "x = [z+n for n in range(N+1)]\n",
    "y = [nbinom.pmf(n, z, theta) for n in range(N+1)]\n",
    "\n",
    "d = DistributionMass(x,y)\n",
    "ax = d.plot()\n",
    "_ = ax.set_xlabel(\"Number of coin flips (N)\")"
   ]
  }
